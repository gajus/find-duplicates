{
  "author": {
    "email": "gajus@gajus.com",
    "name": "Gajus Kuizinas",
    "url": "http://gajus.com"
  },
  "ava": {
    "require": [
      "@babel/register"
    ]
  },
  "description": "Finds duplicate entries in a JavaScript array using an iteratee.",
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/node": "^7.7.0",
    "@babel/plugin-transform-flow-strip-types": "^7.6.3",
    "@babel/preset-env": "^7.7.1",
    "@babel/register": "^7.7.0",
    "ava": "^2.4.0",
    "babel-plugin-istanbul": "^5.2.0",
    "coveralls": "^3.0.7",
    "eslint": "^6.6.0",
    "eslint-config-canonical": "^18.0.0",
    "flow-bin": "^0.112.0",
    "flow-copy-source": "^2.0.8",
    "husky": "^3.0.9",
    "nock": "^11.7.0",
    "nyc": "^14.1.1",
    "semantic-release": "^15.13.30",
    "sinon": "^7.5.0"
  },
  "engines": {
    "node": ">=8.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run test"
    }
  },
  "keywords": [
    "array",
    "duplicates",
    "utility"
  ],
  "main": "./dist/findDuplicates.js",
  "name": "find-duplicates",
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "instrument": false,
    "reporter": [
      "text-lcov"
    ],
    "require": [
      "@babel/register"
    ],
    "sourceMap": false
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:gajus/find-duplicates.git"
  },
  "scripts": {
    "build": "rm -fr ./dist && NODE_ENV=production babel ./src --out-dir ./dist --copy-files --source-maps && flow-copy-source src dist",
    "dev": "NODE_ENV=production babel ./src --out-dir ./dist --copy-files --source-maps --watch",
    "lint": "eslint ./src ./test && flow",
    "test": "NODE_ENV=test ava --serial --verbose"
  },
  "version": "1.0.0"
}
